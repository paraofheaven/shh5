{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "warning",
  "jsRules": {
    "arrow-parens": false,
    "forin": false,
    "max-line-length": [
      false
    ],
    "no-bitwise": false,
    "no-console": false,
    "no-conditional-assignment": false,
    "no-debugger": false,
    "no-empty": false,
    "no-unused-expression": true,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": [
      true,
      "ignore-for-loop"
    ],
    "only-arrow-functions": false,
    "ordered-imports": false,
    "prefer-const": true,
    "prefer-for-of": false,
    "quotemark": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  },
  "rules": {
    "member-ordering": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "max-line-length": [
      false
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "prefer-for-of": false,
    "arrow-parens": false,
    "semicolon": [
      true,
      "always"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "curly": false
  }
}